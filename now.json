{
  "name": "next-stump",
  "version": 2,
  "alias": [
    "stumptownbear.com",
    "www.stumptownbear.com"
  ],
  "env": {
    "MAILGUN_API_KEY": "@mailgun-api-key"
  },
  "builds": [
    { "src": "send-email.js", "use": "@now/node" },
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/send-email", "dest": "send-email.js" },
    { "src": "/portfolio/(?<slug>[^/]+)$", "dest": "/portfolio?slug=$slug" }
  ]
}
